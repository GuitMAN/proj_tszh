@model IEnumerable<string>
        
        <ul>
            <li>@Ajax.ActionLink("Главная", "Index", "User",
                                            new AjaxOptions { UpdateTargetId = "test" })</li>
            @foreach (var link in Model)
            {
                if (link.Equals("Главная")) { continue; }
            <li>@Ajax.RouteLink(link, new { controller = "User", action = "Index", id = link}, new AjaxOptions { UpdateTargetId = "test" })</li>
            }
        </ul>
